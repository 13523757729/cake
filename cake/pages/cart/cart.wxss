.cartnull{
  width:100%;
  height:100vh;
  display:flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
  padding:20rpx;
}
.text1,.text2,.text1,.goList{
  margin-top:40rpx;
}
.text1{
  font-size:16px;
  color:#333;
}
.text2{
  font-size:13px;
  color:#666;
}
.goList{
  width:200rpx;
  height:70rpx;
  border:1px solid rgb(255, 115, 0);
  border-radius:1;
  background-color:transparent;
  color:rgb(255, 115, 0);
  font-size:14px;
  padding:0 20rpx;
  line-height:70rpx;
}
.logo{
  width:40rpx;
  height:40rpx;
}
.myshop{
  font-size:14px;
  background:#fff;
  padding:10rpx 20rpx;
  box-sizing: border-box;
  border-bottom:1px solid #ddd;
}
.checkboxs{
  width:100%;
  height:100%;
  text-align: center;
}
checkbox .wx-checkbox-input{
  width:30rpx;
  height:30rpx;
  border-radius:50%;
  background:transparent;
  border:1px solid #666;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked{
  border: none;
  background: red;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
   border-radius: 50%;/* 圆角 */
   width: 30rpx;/* 选中后对勾大小，不要超过背景的尺寸 */
   height: 30rpx;/* 选中后对勾大小，不要超过背景的尺寸 */
   line-height: 30rpx;
   text-align: center;
   font-size:20rpx; /* 对勾大小 30rpx */
   color:#fff; /* 对勾颜色 白色 */
   background: transparent;
   transform:translate(-50%, -50%) scale(1);
   -webkit-transform:translate(-50%, -50%) scale(1);
}
.cakeitem{
  padding:20rpx;
  background-color:#fff;
  margin-top:20rpx;
}
.cakeitem,.tide,.pricebox,.btns{
  display:flex;
  flex-direction: row;
  align-items: center;
}
.pricebox,.tide{
  align-items: center;
  justify-content: space-between;
}
.pricebox{
  margin-top:30rpx;
}
.checkboxs{
  width:10%;
}
.center{
  width:25%;
}
.rightcom{
  width:65%;
  display:flex;
  flex-direction: column;
}
.cakeimg{
  width:150rpx;
  height:150rpx;
}
.caketitle{
  font-size:16px;
  width:300rpx;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.price{
  color:#f00;
  font-size:16px;
}
.delete,.btns button,.btns text{
  width:60rpx;
  height:60rpx;
}
.btns button,.btns text{
  padding:0;
  line-height: 60rpx;
  text-align: center;
}
.btns button{
  background-color:rgb(234, 235, 235);
  border-radius: 0;
}
.btns button::after{
  border:0;
  border-radius: 0;
}
.total{
  width:100%;
  position:fixed;
  bottom:0;
  background-color:#fff;
}
.total,.checkAll{
  display:flex;
  flex-direction: row;
  align-items: center;
}
.totals{
  width:30%;
  display: flex;
  flex-direction: column;
  text-align: right;
  padding:20rpx;
}
.checkAll{
  width:40%;
  font-size:12px;
  padding-left:35rpx;
  box-sizing: border-box;
}
.totals text{
  font-size:14px;
}
.totalpay{
  color:#f00;
}
.totals text:first-child{
  font-weight:600;
}
.settle{
  width:30%;
  height:100%;
  border: 0;
  background:#f00;
  border-radius:0;
  padding:0;
  color:#fff;
  font-size:16px;
}
.settle::after{
  border-radius:0;
  border:0;
}
.settle::disabled{
  border-radius:0;
  border:0;
}